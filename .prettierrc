{
  "semi": false,
  "singleQuote": true,
  "tailwindConfig": "./tailwind.config.js",
  "trailingComma": "all",
  "plugins": [
    "./node_modules/prettier-plugin-blade/"
  ],
  "overrides": [
    {
      "files": [
        "*.blade.php"
      ],
      "options": {
        "parser": "blade"
      }
    }
  ]
}